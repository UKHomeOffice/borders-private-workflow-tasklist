{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "jest": true,
    "node": true
  },
  "globals": {
    "mount": "readonly",
    "render": "readonly",
    "shallow": "readonly"
  },
  "overrides": [
    {
      "files": ["AccessibilityStatement.jsx"],
      "rules": {
        "max-len": 0
      }
    },
    {
      "files": ["TaskForm.jsx"],
      "rules": {
        "react/static-property-placement": [2, "static public field"]
      }
    },
    {
      "files": ["FormsStartPage.test.jsx", "ShiftPage.test.jsx"],
      "rules": {
        "no-console": 0
      }
    }
  ],
  "parser": "babel-eslint",
  "rules": {
    "arrow-parens": [2, "as-needed"],
    "react/jsx-fragments": 0,
    "react/forbid-prop-types": 0,
    "react/jsx-one-expression-per-line": 0,
    "react/jsx-props-no-spreading": 0
  }
}
