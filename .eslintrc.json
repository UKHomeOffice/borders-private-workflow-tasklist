{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "jest": true,
    "node": true
  },
  "overrides": [
    {
      "files": ["AccessibilityStatement.jsx"],
      "rules": {
        "max-len": 0
      }
    },
    {
      "files": ["TaskForm.jsx"],
      "rules": {
        "react/static-property-placement": [2, "static public field"]
      }
    }
  ],
  "parser": "babel-eslint",
  "rules": {
    "arrow-parens": [2, "as-needed"],
    "react/jsx-fragments": 0,
    "react/forbid-prop-types": 0,
    "react/jsx-one-expression-per-line": 0
  }
}
